{
  "name": "authenticator",
  "expo": {
    "entryPoint": "expo-router/entry"
  },
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "export:web": "expo export --platform web && node scripts/fix-paths.js",
    "electron:dev": "concurrently \"wait-on http://localhost:8081 && NODE_ENV=development ELECTRON_START_URL=http://localhost:8081 electron electron/main.js\" \"expo start --web --port 8081\"",
    "electron:build": "npm run export:web && electron-builder --config electron-builder.json",
    "electron:build:mac": "npm run export:web && electron-builder --config electron-builder.json --mac",
    "electron:build:win": "npm run export:web && electron-builder --config electron-builder.json --win",
    "electron:build:linux": "npm run export:web && electron-builder --config electron-builder.json --linux"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "crypto-js": "^4.2.0",
    "eas-cli": "^16.28.0",
    "electron-store": "^11.0.2",
    "expo": "^54.0.19",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.8",
    "expo-clipboard": "^8.0.7",
    "expo-constants": "~18.0.9",
    "expo-crypto": "^15.0.7",
    "expo-dev-client": "~6.0.16",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-random": "^14.0.1",
    "expo-router": "~6.0.6",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.11",
    "expo-web-browser": "~15.0.7",
    "otpauth": "^9.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-countdown-circle-timer": "^3.2.1",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "simple-icons": "^15.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@electron/rebuild": "^3.7.2",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "~19.1.0",
    "concurrently": "^9.2.1",
    "electron": "^39.2.5",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3",
    "wait-on": "^9.0.3"
  },
  "engines": {
    "node": ">=20.19.4",
    "npm": ">=10.5.2"
  },
  "overrides": {
    "node-forge": "1.3.3"
  },
  "private": true
}
